{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "appstract/laravel-blade-directives": "^0.7",
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "e2kaneko/laravel-chatwork-notifications": "dev-patch",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "~3.3",
        "laravel-notification-channels/webpush": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "^1.0",
        "laravelcollective/html": "5.5.*",
        "league/csv": "^9.0",
        "morrislaptop/laravel-queue-clear": "^1.1",
        "overtrue/laravel-lang": "~3.0",
        "predis/predis": "^1.1",
        "revolution/laravel-amazon-product-api": "^0.4",
        "revolution/laravel-mastodon-api": "^1.3",
        "revolution/laravel-notification-mastodon": "^1.0",
        "revolution/socialite-amazon": "^1.0",
        "tcg/voyager": "^1.0",
        "torann/json-ld": "^0.0.10"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^7.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.0",
        "wnx/laravel-stats": "^1.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "psr-0": {
            "MarketplaceWebServiceProducts_": "lib/amazon-mws-api/src/MarketplaceWebServiceProducts"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        },
        "chatwork": {
            "type": "vcs",
            "url": "https://github.com/kawax/laravel-chatwork-notification"
        }
    }
}
